<table class="ann_table" cellpadding="0" cellspacing="0">
    {% for i,item in paginator %}
        {% if i==5 %}
            <tr>
                <td colspan="5">
                    <script type="text/javascript"><!--
                        google_ad_client = "ca-pub-5949181121748532";
                        /* Leaderboard */
                        google_ad_slot = "2380791311";
                        google_ad_width = 728;
                        google_ad_height = 90;
                        //-->
                        </script>
                        <script type="text/javascript"
                        src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
                    </script>
                </td>
            </tr>
        {% endif %}
        <tr itemscope itemtype="http://schema.org/ApartmentComplex">
            <td>
                <a href="{{ path('DauDauBundle_ann_details', { 'id': item.id, 'title_slug':item.titleslug }) }}" class="ann_link" itemprop="url"><span itemprop="name">{{item.title|truncate(0, 60, '...')|no_title|stripslashes}}</span></a>
            </td>
            <td class="ann_details">
                {{item.added|date('Y-m-d')}}
                <br />
                <span class="italic font10">{{item.added|date('Y-m-d H:i:s')|days_ago}}</span>
            </td>
            <td class="ann_details">
                {% if item.nrrooms == 0 %}1{% else %}{{item.nrrooms}}{% endif %} camere
            </td>
            <td class="ann_details">
                {{raioane[item.raion]}}
            </td>
            <td class="ann_details">
                {{item.price}} euro
            </td>
        </tr>
    {% else %}
        <tr>
            <td>
                La moment nu avem nici un rezultat pentru aceasta cautare.
            </td>
        </tr>
    {% endfor %}
</table>